{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "strictBindCallApply": true,
    "strict": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "akita-ng-odata-service": [
        "projects/akita-ng-odata-service/src/public-api",
        "dist/akita-ng-odata-service"
      ],
      "akita-ng-odata-service/*": [
        "dist/akita-ng-odata-service/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": false
  }
}